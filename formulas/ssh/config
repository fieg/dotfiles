Include ~/.dotfiles/formulas/ssh/conf.d/*
Include conf.d/*

ServerAliveInterval 300
ServerAliveCountMax 36
IdentitiesOnly yes

Host *
   UseKeychain yes
   IdentityFile ~/.ssh/id_ed25519
   IdentityFile ~/.ssh/id_rsa
   AddKeysToAgent yes

Host 127.0.0.1
   StrictHostKeyChecking no
   UserKnownHostsFile=/dev/null

Host localhost
   StrictHostKeyChecking no
   UserKnownHostsFile=/dev/null
